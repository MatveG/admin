{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google",
        "plugin:vue/essential",
        "@vue/standard"
    ],
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "rules": {
        "linebreak-style": "off",
        "semi": "off",
        "require-jsdoc": "off",
        "max-len": "off"
    }
}
