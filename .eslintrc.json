{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "google",
        "plugin:vue/essential",
        "@vue/standard"
    ],
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "rules": {
        "require-jsdoc": "off",
        "linebreak-style": "off",
        "semi": [2, "always"],
        "max-len": ["error", 100]
    }
}
